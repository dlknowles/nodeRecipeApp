<h1>Subscriber Data for <%= subscriber.name %></h1>

<table class="table">
    <tr>
        <th>Name</th>
        <td><%= subscriber.name %></td>
    </tr>
    <tr>
        <th>Email</th>
        <td><%= subscriber.email %></td>
    </tr>
    <tr>
        <th>Zip Code</th>
        <td><%= subscriber.zipCode %></td>
    </tr>
    
</table>

<% if (subscriber.courses && subscriber.courses.length > 0) { %>
<h4>Courses</h4>
<ul>
    <% subscriber.courses.forEach(course => { %>
    <li><%= course.title %></li>
    <% }) %>
</ul>
<% } %>
